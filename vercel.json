{
  "redirects": [
    {
      "source": "/ga4-analytics",
      "destination": "/ga4-analytics/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/ga4-analytics/",
      "destination": "https://ga-4-flax.vercel.app/"
    },
    {
      "source": "/ga4-analytics/:path+",
      "destination": "https://ga-4-flax.vercel.app/:path+"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
